{
  "name": "brethrenfgm-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "test": "vitest",
    "prisma:seed": "node --loader ts-node/esm --no-warnings prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.17.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "helmet": "7.0.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "socket.io": "4.7.5",
    "zod": "3.23.8",
    "express-async-errors": "3.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/multer": "1.4.11",
    "@types/node": "20.12.12",
    "@vitest/coverage-c8": "1.6.0",
    "eslint": "9.4.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.2.5",
    "prisma": "5.17.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0",
    "ts-node": "10.9.2"
  }
}
